SELECT list_all.fdl Date, COALESCE(announce.mlu,0) 'announce', COALESCE(arch.mlu,0) 'arch', COALESCE(board.mlu,0) 'board', COALESCE(enginedevel.mlu,0) 'engine-devel', COALESCE(infra.mlu,0) 'infra', COALESCE(nodedevel.mlu,0) 'node-devel', COALESCE(users.mlu,0) 'users', COALESCE(vdsmdevel.mlu,0) 'vdsm-devel', COALESCE(nodepatches.mlu,0) 'node-patches', COALESCE(vdsmpatches.mlu,0) 'vdsm-patches', COALESCE(enginepatches.mlu,0) 'engine-patches'  FROM 
(select LEFT(first_date, 10) fdl from messages WHERE first_date NOT LIKE '%1979%' group by fdl order by first_date) list_all
LEFT JOIN 
(select LEFT(first_date, 10) fdl, count(mailing_list_url) mlu from messages m where mailing_list_url LIKE '%announce%' group by fdl order by first_date) announce
on list_all.fdl = announce.fdl
LEFT JOIN 
(select LEFT(first_date, 10) fdl, count(mailing_list_url) mlu from messages m where mailing_list_url LIKE '%arch%' group by fdl order by first_date) arch
on list_all.fdl = arch.fdl
LEFT JOIN 
(select LEFT(first_date, 10) fdl, count(mailing_list_url) mlu from messages m where mailing_list_url LIKE '%board%' group by fdl order by first_date) board
on list_all.fdl = board.fdl
LEFT JOIN 
(select LEFT(first_date, 10) fdl, count(mailing_list_url) mlu from messages m where mailing_list_url LIKE '%engine-devel%' group by fdl order by first_date) enginedevel
on list_all.fdl = enginedevel.fdl
LEFT JOIN 
(select LEFT(first_date, 10) fdl, count(mailing_list_url) mlu from messages m where mailing_list_url LIKE '%infra%' group by fdl order by first_date) infra
on list_all.fdl = infra.fdl
LEFT JOIN 
(select LEFT(first_date, 10) fdl, count(mailing_list_url) mlu from messages m where mailing_list_url LIKE '%node-devel%' group by fdl order by first_date) nodedevel
on list_all.fdl = nodedevel.fdl
LEFT JOIN 
(select LEFT(first_date, 10) fdl, count(mailing_list_url) mlu from messages m where mailing_list_url LIKE '%users%' group by fdl order by first_date) users
on list_all.fdl = users.fdl
LEFT JOIN 
(select LEFT(first_date, 10) fdl, count(mailing_list_url) mlu from messages m where mailing_list_url LIKE '%vdsm-devel%' group by fdl order by first_date) vdsmdevel
on list_all.fdl = vdsmdevel.fdl
LEFT JOIN 
(select LEFT(first_date, 10) fdl, count(mailing_list_url) mlu from messages m where mailing_list_url LIKE '%node-patches%' group by fdl order by first_date) nodepatches
on list_all.fdl = nodepatches.fdl
LEFT JOIN 
(select LEFT(first_date, 10) fdl, count(mailing_list_url) mlu from messages m where mailing_list_url LIKE '%engine-patches%' group by fdl order by first_date) enginepatches
on list_all.fdl = enginepatches.fdl
LEFT JOIN 
(select LEFT(first_date, 10) fdl, count(mailing_list_url) mlu from messages m where mailing_list_url LIKE '%vdsm-patches%' group by fdl order by first_date) vdsmpatches
on list_all.fdl = vdsmpatches.fdl;
